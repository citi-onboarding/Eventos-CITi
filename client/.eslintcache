ak\n        case 4:\n            month = \"abril\"\n            break\n        case 5:\n            month = \"maio\"\n            break\n        case 6:\n            month = \"junho\"\n            break;\n        case 7:\n            month = \"julho\"\n            break;\n        case 8:\n            month = \"agosto\"\n            break\n        case 9:\n            month = \"setembro\"\n            break\n        case 10:\n            month = \"outubro\"\n            break\n        case 11:\n            month = \"novembro\"\n            break\n        case 12:\n            month = \"dezembro\"\n            break\n    }\n    date = eventDate.getDay() + \" de \" + month + \" de \" + eventDate.getFullYear()\n\n  return (\n    <article className=\"blogCard\" id=\"blogCard\">\n        <img src={props.image} alt=\"Blog Card Image\" className=\"BlogCardImage\" />\n        <div className=\"content\">\n          <p className=\"Date\">Eventos / {date} </p>\n          <h4>{props.name}</h4>\n          <p className=\"description\"> {props.description} </p>\n          <a href={props.link} className=\"ReadMoreMobile\">Leia  &nbsp; <Vector id=\"blogVector\"/></a>\n          <a href={props.link} className=\"ReadMoreWeb\">Leia o texto completo <Vector/></a>\n        </div>\n    </article>\n  );\n}","/home/guilherme/citi/Eventos-CITi/client/src/components/SubscriptionModal/SubscriptionModal.jsx",["80","81","82","83","84","85","86"],"import './styles.css';\nimport { useState, useEffect, useRef } from 'react';\nimport axios from 'axios';\nimport calendar from './images/calendar.svg'\nimport gps from './images/gps.svg'\nfunction SubscriptionModal(props) {\n    const node = useRef();\n    const monthString = ((date) => {\n        switch (date.getMonth()) {\n            case 0:\n                return \"janeiro\"\n            case 1:\n                return \"fevereiro\"\n            case 2:\n                return \"março\"\n            case 3:\n                return \"abril\"\n       